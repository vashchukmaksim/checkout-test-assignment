<!doctype html>
<html lang="en">
<head>

    <!-- Title -->
    <title>Customer Feedback</title>

    <!-- Description -->
    <meta name="description" content="Manage description" />

    <!-- Meta -->
	<meta name="description" content="description" />
	<meta charset="utf-8" />
	<meta http-equiv="x-ua-compatible" content="ie=edge" />
	<meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="robots" content="index" />

    <!-- Mobile icons -->
    <link rel="apple-touch-icon" sizes="180x180" href="{{ webpackConfig.output.publicPath }}favicon/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ webpackConfig.output.publicPath }}favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ webpackConfig.output.publicPath }}favicon/favicon-16x16.png">
    <link rel="manifest" href="{{ webpackConfig.output.publicPath }}favicon/site.webmanifest">
    <link rel="mask-icon" href="{{ webpackConfig.output.publicPath }}favicon/safari-pinned-tab.svg" color="#2051cb">
    <link rel="shortcut icon" href="{{ webpackConfig.output.publicPath }}favicon/favicon.ico">

    <!-- Icon colors -->
    <meta name="msapplication-TileColor" content="#2051cb">
    <meta name="msapplication-config" content="{{ webpackConfig.output.publicPath }}favicon/browserconfig.xml">
    <meta name="theme-color" content="#2051cb">

    <!-- Facebook -->
    <meta property="og:type" content="website">
    <meta property="og:url" content="">
    <meta property="og:title" content="">
    <meta property="og:image" content="/static/public/images/social-cover.jpg">
    <meta property="og:description" content="">
    <meta property="og:site_name" content="">
    <meta property="og:locale" content="en_US">

    <!-- Twitter -->
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="">
    <meta name="twitter:creator" content="">
    <meta name="twitter:url" content="">
    <meta name="twitter:title" content="">
    <meta name="twitter:description" content="">
    <meta name="twitter:image" content="/static/public/images/social-cover.jpg">

    <!-- Favicon -->
	{{#if webpackConfig.output.publicPath }}
	<link rel="shortcut icon" href="{{ webpackConfig.output.publicPath }}images/favicon.png" />
	{{/if}}

    <!-- Manifest -->
	{{#if webpackConfig.output.publicPath }}
	<link rel="manifest" href="{{ webpackConfig.output.publicPath }}manifest.json" />
	{{/if}}
</head>

<body>
	<div id="app"></div>
</body>

</html>
